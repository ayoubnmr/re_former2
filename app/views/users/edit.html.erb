<%= form_for @user do |u|%>
<ul>
      <% @user.errors.each do |error, msg| %>
        <li><%= "#{error} - #{msg}" %></li>
    </ul>
  <% end %>
  
  <%= u.label :username %>
  <%= u.text_field :username%>
  <%= u.label :email%>
  <%= u.text_field :email%>
  <%= u.label :password%>
  <%= u.text_field :password, :type =>"password"%>
  <%= u.submit "Create"%>
  <%end%>